{
  "name": "pollen-solutions/wp-hook",
  "description": "Pollen Wordpress Hook Component.",
  "homepage": "https://github.com/milkcreation/pollen-wp-hook.git",
  "keywords": [
    "pollen-solutions",
    "component",
    "wordpress",
    "hook",
    "post"
  ],
  "time": "2021-09-15",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "composer/composer": "^2.0",
    "composer/installers": "~1.0",
    "pollen-solutions/support": "^1.0",
    "pollen-solutions/wp-post": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\WpHook\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\WpHook\\Test\\": "tests"
    }
  },
  "suggest": {
    "pollen-solutions/container" : "Pollen Container Component - PSR-11 ready Dependencies Injection Container."
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
